msgstr "{{date}} – {{status}}"

#. # States
#: download-stopped
msgid "Stopped"
msgstr "Спрян"

#: download-failed
msgctxt "download-failed"
msgid "Download failed"
msgstr "Грешка при изтегляне"

#: open_downloaded_file
msgid "Open"
msgstr "Отваряне"

#: share_downloaded_file
msgid "Share"
msgstr "Споделяне"

#: wallpaper_downloaded_file
msgid "Set as wallpaper"
msgstr "Поставяне като тапет"

#: ringtone_downloaded_file
msgid "Set as ringtone"
msgstr "Задаване като звук за звънене"

#: cancel_downloaded_file
msgctxt "cancel_downloaded_file"
msgid "Cancel"
msgstr "Прекъсване"
