#. # Bluetooth :: Confirmation Dialog
#: bluetooth
msgid "Bluetooth"
msgstr "Bluetooth"

#: confirmation
msgid "Confirmation"
msgstr "Nkwenye"

#: bluetooth-status-description
msgid "Bluetooth is disabled"
msgstr "Agbanyụrụ Bluetooth"

#: turn-bluetooth-on
msgid "Do you want to turn Bluetooth on?"
msgstr "Ị chọrọ ịgbanwu Bluetooth?"

#: cancel
msgid "Cancel"
msgstr "Kagbuo"

#: turn-on
msgid "Turn On"
msgstr "Gbanwuo"

#: error-transfer-title
msgid "Bluetooth file transfer failed"
msgstr "Nnyefe faịlụ Bluetooth agaghị"

#: error-transfer-settings
msgid ""
"Bluetooth file transfer failed. Check that the Bluetooth settings are "
"correct."
msgstr "Nnyefe faịlụ Bluetooth agaghị. Lelee na ntọala Bluetooth ziri ezi."

#: ok
msgid "OK"
msgstr "ỌDỊMMA"

#. # Bluetooth :: Devices Panel
#: bluetooth-paired-devices
msgid "Paired devices"
msgstr "Ngwaọrụ ndị amakọrọ"

#: bluetooth-devices-in-area
msgid "Devices in the area"
msgstr "Ngwaọrụ ndị dị na mpaghara a"

#: device-status-tap-connect
msgid "Tap to connect"
msgstr "Pịa iji jikọọ"

#: device-status-pairing
msgid "Pairing with device…"
msgstr "Na-amakọ yana ngwaọrụ…"

#: device-status-waiting
msgid "Waiting for other device…"
msgstr "Na eche ngwaọrụ nke ọzọ…"

#: device-status-paired
msgid "Paired"
msgstr "Amakọrọ"

#: device-status-connecting
msgid "Connecting…"
msgstr "Na-ejikọ…"

#: device-status-connected
msgid "Connected"
msgstr "Ejikọrọ"

#: search-for-device
msgid "Searching for devices…"
msgstr "Na-achọgharị ngwaọrụ…"

#: search-device
msgid "Search for devices"
msgstr "Chọgharịa ngwaọrụ"

#: unnamed-device
msgid "Unknown device"
msgstr "Ngwaọrụ amaghị"

#: error-pair-title
msgid "Unable to pair devices"
msgstr "Enwenwughi ike ịmakọ ngwaọrụ"

#: error-pair-pincode
msgid "Unable to pair with the device. Check that the PIN is correct."
msgstr "Enwenwughi ike ịmakọ yana ngwaọrụ ahụ. Lelee na PIN ahụ ziri ezi."

#: error-pair-toofast
msgid "Unable to pair with the device. Too many requests."
msgstr "Enwenwughi ike ịmakọ yana ngwaọrụ ahụ. Ọtụtụ arịrịọ."

#. # Bluetooth :: Pair View
#: pair
msgid "Pair"
msgstr "Makọọ"

#: active-pair
msgid "Confirm devices to pair"
msgstr "Kwenye na ngwaọrụ iji makọọ"

#: active-pair-confirmation
msgid ""
"To pair with {{device}}, make sure that both devices currently have the "
"following PIN displayed:"
msgstr ""
"Iji makọọ yana {{device}}, gbaa mbọ hụ na ngwaọrụ abụọ ahụ nwere PIN "
"egosipụtara:"

#: active-pair-pincode
msgid "To pair with {{device}}, enter the PIN on the device."
msgstr "Iji makọọ yana {{device}}, tinye PIN dị na ngwaọrụ ahụ."

#: active-pair-passkey
msgid "To pair with {{device}}, enter the passkey on the device."
msgstr "Iji makọọ yana {{device}}, tinye igodo ntughe dị na ngwaọrụ ahụ."

#: passive-pair
msgctxt "passive-pair"
msgid "Bluetooth pairing request"
msgstr "Arịrịọ mmakọ Bluetooth"

#: passive-pair-confirmation
msgid ""
"{{device}} would like to pair with this phone. To accept, make sure that "
"both devices currently have the following PIN displayed:"
msgstr ""
"{{device}} ga-achọ ịmakọ na ekwentị a. Iji nabata, gbaa mbọ hụ na ngwaọrụ "
"abụọ ahụ nwere PIN ndị a egosipụtara:"

#: passive-pair-pincode
msgid ""
"{{device}} would like to pair with this phone. To accept, enter the PIN on "
"the device."
msgstr ""
"{{device}} ga-achọ ịmakọ na ekwentị a. Iji nabata, tinye PIN dị na ngwaọrụ "
"ahụ."

#: passive-pair-passkey
msgid ""
"{{device}} would like to pair with this phone. To accept, enter the passkey "
"on the device."
msgstr ""
"{{device}} ga-achọ ịmakọ na ekwentị a. Iji nabata, tinye igodo ntughe dị na "
"ngwaọrụ ahụ."

#: pair-view-title
msgid "Bluetooth Request From"
msgstr "Arịrịọ Bluetooth Si Na"

#. # Bluetooth :: Pair Manager
#: bluetooth-pairing-request-now-title
msgctxt "bluetooth-pairing-request-now-title"
msgid "Bluetooth pairing request"
msgstr "Arịrịọ mmakọ Bluetooth"

#: pairing-request-timeout-title
msgid "Bluetooth Pairing Request"
msgstr "Arịrịọ Mmakọ Bluetooth"

#: pairing-request-timeout-description
msgid "Bluetooth pairing request expired. Try to pair again."
msgstr "Oge arịrịọ mmakọ Bluetooth agafeela. Nwaa ịmakọ ọzọ."
