#. # Prompt that appears when a user tries to stop a download.
#: gaia-grid-stop-download-title
msgid "Stop {{name}}"
msgstr "Arrêter {{name}}"

#: gaia-grid-stop-download-body
msgid "Do you want to stop the download in progress?"
msgstr "Voulez-vous arrêter le téléchargement en cours ?"

#: gaia-grid-stop-download-action
msgid "Stop"
msgstr "Arrêter"

#. # Prompt that appears when a user tries to resume a download.
#: gaia-grid-resume-download-title
msgid "Restart Download"
msgstr "Redémarrer le téléchargement"

#: gaia-grid-resume-download-body
msgid "Do you want to download {{name}}?"
msgstr "Voulez-vous télécharger {{name}} ?"

#: gaia-grid-resume-download-action
msgid "Download"
msgstr "Télécharger"

#: gaia-grid-cancel
msgid "Cancel"
msgstr "Annuler"

#. # LOCALIZATION NOTE(*.ariaLabel): The following strings are spoken by screen
#. # readers and not shown on the screen.
#: gaia-grid-toggle-collapsed.ariaLabel
msgid "Expand group"
msgstr "Développer le groupe"

#: gaia-grid-toggle-expanded.ariaLabel
msgid "Collapse group"
msgstr "Réduire le groupe"

#. # Hint that appears in edit mode for groups with no title set.
#: gaia-grid-enter-group-name
msgid "Enter a group name"
msgstr "Saisir un nom de groupe"
