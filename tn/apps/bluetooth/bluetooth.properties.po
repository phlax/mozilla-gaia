#. # Bluetooth :: Confirmation Dialog
#: bluetooth
msgid "Bluetooth"
msgstr "Bluetooth"

#: confirmation
msgid "Confirmation"
msgstr "Tlhomamisa"

#: bluetooth-status-description
msgid "Bluetooth is disabled"
msgstr "Bluetooth e nonofolotswe "

#: turn-bluetooth-on
msgid "Do you want to turn Bluetooth on?"
msgstr "A o batla go tshuba Bluetooth?"

#: cancel
msgid "Cancel"
msgstr "Sutlha"

#: turn-on
msgid "Turn On"
msgstr "Tshuba"

#: error-transfer-title
msgid "Bluetooth file transfer failed"
msgstr "Feile ya Bluetooth e tlhaetse"

#: error-transfer-settings
msgid ""
"Bluetooth file transfer failed. Check that the Bluetooth settings are "
"correct."
msgstr "Thomelo ya feile e tlhaetse. Netefatsa gore Bluetooth e siame."

#: ok
msgid "OK"
msgstr "OK"

#. # Bluetooth :: Devices Panel
#: bluetooth-paired-devices
msgid "Paired devices"
msgstr "Didirisiwa tse di berikisanyang"

#: bluetooth-devices-in-area
msgid "Devices in the area"
msgstr "Didirisiwa tse di mabapi"

#: device-status-tap-connect
#, fuzzy
msgid "Tap to connect"
msgstr "Tobetsa go dirisa"

#: device-status-pairing
msgid "Pairing with device…"
msgstr "Golaganya didirisiwa"

#: device-status-waiting
msgid "Waiting for other device…"
msgstr "Emela didirisiwa..."

#: device-status-paired
msgid "Paired"
msgstr "Kgolagano"

#: device-status-connecting
msgid "Connecting…"
msgstr "Ya Kitlanya..."

#: device-status-connected
msgid "Connected"
msgstr "E kitlanye"

#: search-for-device
msgid "Searching for devices…"
msgstr "E kèlèka maanò"

#: search-device
msgid "Search for devices"
msgstr "Kèlèka maanò"

#: unnamed-device
msgid "Unknown device"
msgstr "Maanò a a sa itseweng "

#: error-pair-title
msgid "Unable to pair devices"
msgstr "Gana sebedi sa maanò"

#: error-pair-pincode
msgid "Unable to pair with the device. Check that the PIN is correct."
msgstr "Gana sebedi sa maanò. Netefatsa nomoro ya sephiri gore e siame."

#: error-pair-toofast
msgid "Unable to pair with the device. Too many requests."
msgstr "Gana sebedi sa maanò. Kopo di dintsi."

#. # Bluetooth :: Pair View
#: pair
msgid "Pair"
msgstr "Golaganya"

#: active-pair
msgid "Confirm devices to pair"
msgstr "Tlhomamisa maanò a kgolaganyo"

#: active-pair-confirmation
msgid ""
"To pair with {{device}}, make sure that both devices currently have the "
"following PIN displayed:"
msgstr ""
"Go pana le {{device}}, netefatsa gore didiriswa tsotlhe ga jaana di na le "
"PIN e latelang e supilweng:"

#: active-pair-pincode
msgid "To pair with {{device}}, enter the PIN on the device."
msgstr "Go pana le {{device}}, tsenya PIN mo sedirisiweng."

#: active-pair-passkey
msgid "To pair with {{device}}, enter the passkey on the device."
msgstr "Go pana le {{device}}, tsenya taetsi khunololamoraba mo sedirisweng."

#: passive-pair
msgctxt "passive-pair"
msgid "Bluetooth pairing request"
msgstr "Kopo ya kgolagano ya Blueetooth"

#: passive-pair-confirmation
msgid ""
"{{device}} would like to pair with this phone. To accept, make sure that "
"both devices currently have the following PIN displayed:"
msgstr ""
"{{device}} eletsa go pana le founu e. Go dumalana, netefatsa gore didiriswa "
"tsotlhe ga jaana di na le PIN e supilweng:"

#: passive-pair-pincode
msgid ""
"{{device}} would like to pair with this phone. To accept, enter the PIN on "
"the device."
msgstr ""
"{{device}} e batla go pana le founu e. Go amogela, tsenya PIN mo sedirisweng."

#: passive-pair-passkey
msgid ""
"{{device}} would like to pair with this phone. To accept, enter the passkey "
"on the device."
msgstr ""
"{{device}} e eletsa go pana le founu e. Go amogela, tsenya taetsi "
"khunololamoraba mo sedirisweng."

#: pair-view-title
msgid "Bluetooth Request From"
msgstr "Kopo ya Bluetooth go tsweng"

#. # Bluetooth :: Pair Manager
#: bluetooth-pairing-request-now-title
msgctxt "bluetooth-pairing-request-now-title"
msgid "Bluetooth pairing request"
msgstr "Kopo ya kgolagano ya Bluetooth"

#: pairing-request-timeout-title
msgid "Bluetooth Pairing Request"
msgstr "Ikopelo ya kgolagano ya Bluetooth"

#: pairing-request-timeout-description
msgid "Bluetooth pairing request expired. Try to pair again."
msgstr "Kopo ya kgolagano ya Bluetooth e fedile. Leka go golaganya gape."
