#. # Bluetooth :: Confirmation Dialog
#: bluetooth
msgid "Bluetooth"
msgstr "Bluetooth"

#: confirmation
msgid "Confirmation"
msgstr "Berrespena"

#: bluetooth-status-description
msgid "Bluetooth is disabled"
msgstr "Bluetooth-a desgaituta dago"

#: turn-bluetooth-on
msgid "Do you want to turn Bluetooth on?"
msgstr "Bluetooth-a piztu nahi duzu?"

#: cancel
msgid "Cancel"
msgstr "Utzi"

#: turn-on
msgid "Turn On"
msgstr "Piztu"

#: error-transfer-title
msgid "Bluetooth file transfer failed"
msgstr "Huts egin du Bluetooth bidezko fitxategi-transferentziak"

#: error-transfer-settings
msgid ""
"Bluetooth file transfer failed. Check that the Bluetooth settings are "
"correct."
msgstr ""
"Huts egin du Bluetooth bidezko fitxategi-transferentziak. Egiaztatu "
"Bluetooth-ezarpenak zuzenak direla."

#: ok
msgid "OK"
msgstr "Ados"

#. # Bluetooth :: Devices Panel
#: bluetooth-paired-devices
msgid "Paired devices"
msgstr "Parekatutako gailuak"

#: bluetooth-devices-in-area
msgid "Devices in the area"
msgstr "Inguruko gailuak"

#: device-status-tap-connect
msgid "Tap to connect"
msgstr "Sakatu konektatzeko"

#: device-status-pairing
msgid "Pairing with device…"
msgstr "Gailuarekin parekatzen…"

#: device-status-waiting
msgid "Waiting for other device…"
msgstr "Beste gailuari itxoiten…"

#: device-status-paired
msgid "Paired"
msgstr "Parekatuta"

#: device-status-connecting
msgid "Connecting…"
msgstr "Konektatzen…"

#: device-status-connected
msgid "Connected"
msgstr "Konektatuta"

#: search-for-device
msgid "Searching for devices…"
msgstr "Gailuak bilatzen…"

#: search-device
msgid "Search for devices"
msgstr "Bilatu gailuak"

#: unnamed-device
msgid "Unknown device"
msgstr "Gailu ezezaguna"

#: error-pair-title
msgid "Unable to pair devices"
msgstr "Ezin dira gailuak parekatu"

#: error-pair-pincode
msgid "Unable to pair with the device. Check that the PIN is correct."
msgstr "Ezin da gailuarekin parekatu. Egiaztatu PINa zuzena dela."

#: error-pair-toofast
msgid "Unable to pair with the device. Too many requests."
msgstr "Ezin da gailuarekin parekatu. Eskaera gehiegi."

#. # Bluetooth :: Pair View
#: pair
msgid "Pair"
msgstr "Parekatzea"

#: active-pair
msgid "Confirm devices to pair"
msgstr "Berretsi gailuak parekatzea"

#: active-pair-confirmation
msgid ""
"To pair with {{device}}, make sure that both devices currently have the "
"following PIN displayed:"
msgstr ""
"{{device}} gailuarekin parekatzeko, ziurtatu bi gailuek ondorengo PINa "
"bistan dutela:"

#: active-pair-pincode
msgid "To pair with {{device}}, enter the PIN on the device."
msgstr "{{device}} gailuarekin parekatzeko, sartu gailuko PINa."

#: active-pair-passkey
msgid "To pair with {{device}}, enter the passkey on the device."
msgstr "{{device}} gailuarekin parekatzeko, sartu gailuko pasagakoa."

#: passive-pair
msgctxt "passive-pair"
msgid "Bluetooth pairing request"
msgstr "Bluetooth parekatze-eskaera"

#: passive-pair-confirmation
msgid ""
"{{device}} would like to pair with this phone. To accept, make sure that "
"both devices currently have the following PIN displayed:"
msgstr ""
"{{device}} gailuak telefono honekin parekatu nahi du. Onartzeko, ziurtatu bi "
"gailuek ondorengo PINa bistan dutela:"

#: passive-pair-pincode
msgid ""
"{{device}} would like to pair with this phone. To accept, enter the PIN on "
"the device."
msgstr ""
"{{device}} gailuak telefono honekin parekatu nahi du. Onartzeko, sartu "
"gailuko PINa."

#: passive-pair-passkey
msgid ""
"{{device}} would like to pair with this phone. To accept, enter the passkey "
"on the device."
msgstr ""
"{{device}} gailuak telefono honekin parekatu nahi du. Onartzeko, sartu "
"gailuko pasagakoa."

#: pair-view-title
msgid "Bluetooth Request From"
msgstr "Bluetooth eskaera hemendik"

#. # Bluetooth :: Pair Manager
#: bluetooth-pairing-request-now-title
msgctxt "bluetooth-pairing-request-now-title"
msgid "Bluetooth pairing request"
msgstr "Bluetooth parekatze-eskaera"

#: pairing-request-timeout-title
msgid "Bluetooth Pairing Request"
msgstr "Bluetooth parekatze-eskaera"

#: pairing-request-timeout-description
msgid "Bluetooth pairing request expired. Try to pair again."
msgstr "Bluetooth parekatze-eskaera iraungi da. Saiatu berriro parekatzen."
